        app:layout_constraintStart_toEndOf="@+id/Month"
        app:layout_constraintTop_toBottomOf="@+id/Title"
        app:layout_constraintVertical_bias="0.100000024" />

    <EditText
        android:id="@+id/Day"
        android:layout_width="88dp"
