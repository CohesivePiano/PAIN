        android:hint="@string/edit_message15"
        android:inputType="textPersonName"
        android:textSize="12.5sp"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="616dp" />

    <EditText
        android:id="@+id/emailpassword"
        android:layout_width="88dp"
        android:layout_height="42dp"
        android:ems="10"
        android:gravity="center"
        android:hint="@string/edit_message20"
        android:inputType="textPersonName"
        android:textSize="11.5sp"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="616dp" />

    <EditText
        android:id="@+id/ZipPostCode"
        android:layout_width="88dp"
        android:layout_height="42dp"
        android:ems="10"
        android:gravity="center"
        android:hint="@string/edit_message14"
        android:inputType="textPersonName"
        android:textSize="12.5sp"
        tools:layout_editor_absoluteX="84dp"
        tools:layout_editor_absoluteY="557dp" />

    <EditText
        android:id="@+id/StateProvince"
        android:layout_width="88dp"
        android:layout_height="42dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:ems="10"
        android:gravity="center"
        android:hint="@string/edit_message12"
        android:inputType="textPersonName"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Country"
        app:layout_constraintStart_toEndOf="@+id/City"
        app:layout_constraintTop_toBottomOf="@+id/Address1"
        app:layout_constraintVertical_bias="0.100000024" />

